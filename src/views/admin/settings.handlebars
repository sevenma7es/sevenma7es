            <input type="text" name="home_section2" placeholder="Escribe aquí" class="text-base rounded-full text-[var(--main-text-light)] flex-1 px-2 py-4 shadow-[0px 15px 30px -4px rgba(16, 24, 40, 0.03)] md:shadow-none !bg-white md:bg-transparent shadow-none placeholder:text-gray-400 focus:outline-none w-full border" id="home_section2" value="{{settingsData.home_section2}}" />
